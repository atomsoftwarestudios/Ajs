<configuration>

  <system.webServer>

    <security>
      <!--
      <ipSecurity allowUnlisted="false" denyAction="NotFound">
        <add allowed="true" ipAddress="193.165.117.166" subnetMask="255.255.255.255"/>
        <add allowed="true" ipAddress="10.0.0.0" subnetMask="255.0.0.0"/>
        <add allowed="true" ipAddress="192.168.0.0" subnetMask="255.255.0.0"/>
        <add allowed="true" ipAddress="172.16.0.0" subnetMask="255.240.0.0"/>
        <add allowed="true" ipAddress="127.0.0.1" subnetMask="255.255.255.255"/>
      </ipSecurity>
      -->
    </security>

    <staticContent>
      <remove fileExtension=".manifest" />
      <mimeMap fileExtension=".manifest" mimeType="text/cache-manifest" />
      <remove fileExtension=".ts"/>
      <mimeMap fileExtension=".ts" mimeType="text/typescript"/>
    </staticContent>

    <rewrite>
      <rules>
        <rule name="DynamicRewrite" enabled="true" stopProcessing="true">
          <match url=".*" ignoreCase="true" />
          <conditions>
            <add input="{REQUEST_FILENAME}" pattern="/*\.manifest$" negate="true" />
            <add input="{REQUEST_FILENAME}" pattern="/*\.js$" negate="true" />
            <add input="{REQUEST_FILENAME}" pattern="/*\.ts$" negate="true" />
            <add input="{REQUEST_FILENAME}" pattern="/*\.map$" negate="true" />
            <add input="{REQUEST_FILENAME}" pattern="/*\.ico$" negate="true" />
            <add input="{REQUEST_FILENAME}" pattern="/*\.png$" negate="true" />
            <add input="{REQUEST_FILENAME}" pattern="/*\.jpg$" negate="true" />
            <add input="{REQUEST_FILENAME}" pattern="/*\.jpeg$" negate="true" />
            <add input="{REQUEST_FILENAME}" pattern="/*\.htm$" negate="true" />
            <add input="{REQUEST_FILENAME}" pattern="/*\.html$" negate="true" />
            <add input="{REQUEST_FILENAME}" pattern="/*\.json$" negate="true" />
            <add input="{REQUEST_FILENAME}" pattern="/*\.css$" negate="true" />
          </conditions>
          <action type="Rewrite" url="/index.html" />
        </rule>
      </rules>
    </rewrite>

    <!--
    <modules>
      <add name="HTTPAuthentication" type="HoldEventCom.HTTPAuthentication"/>
    </modules>
      -->

  </system.webServer>

  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>

</configuration>